    <script>
        function headerWidth() {
            var wrapper = document.querySelector("div.wrapper");
            var header = document.querySelector("header");

            if (wrapper && header) {
                var t = wrapper.offsetWidth;
                header.style.marginLeft = (-t / 2) + "px";
            }
        }

        document.addEventListener("DOMContentLoaded", function () {
            headerWidth();
        });

        window.addEventListener("resize", function () {
            headerWidth();
        });

        document.addEventListener("click", function (event) {
            var menuButton = event.target.closest("a.openMenu");

            if (menuButton) {
                event.preventDefault();
                menuButton.classList.add("activated");

                var closeImageSrc = menuButton.getAttribute("data-close");
                var menuImage = menuButton.querySelector("img");
                if (menuImage) {
                    menuImage.setAttribute("src", closeImageSrc);
                }

                var wrapperElement = document.querySelector(".wrapper");
                if (wrapperElement) {
                    var remainingWidth = window.innerWidth - (wrapperElement.offsetLeft + wrapperElement.offsetWidth);
                    document.querySelector(".nav-closer").style.display = "block";
                    var mainNavigation = document.querySelector(".main-nav");
                    if (mainNavigation) {
                        mainNavigation.style.right = remainingWidth + "px";
                        mainNavigation.style.top = "72px";
                        mainNavigation.classList.add("opened");
                    }
                }
            }

            if (event.target.matches("a.openMenu.activated, .nav-closer")) {
                event.preventDefault();
                var activatedMenuButton = document.querySelector("a.openMenu.activated");
                if (activatedMenuButton) {
                    activatedMenuButton.classList.remove("activated");

                    var openImageSrc = activatedMenuButton.getAttribute("data-open");
                    var activatedMenuImage = activatedMenuButton.querySelector("img");
                    if (activatedMenuImage) {
                        activatedMenuImage.setAttribute("src", openImageSrc);
                    }
                }

                document.querySelector(".nav-closer").style.display = "none";
                var mainNavigation = document.querySelector(".main-nav");
                if (mainNavigation) {
                    mainNavigation.style.top = "-100%";
                }
            }

            if (event.target.matches("a.openSearch")) {
                event.preventDefault();
                var wrapperElement = document.querySelector(".wrapper");
                if (wrapperElement) {
                    var remainingWidth = window.innerWidth - (wrapperElement.offsetLeft + wrapperElement.offsetWidth);
                    var searchDiv = document.querySelector("div.search");
                    if (searchDiv) {
                        searchDiv.style.right = remainingWidth + "px";
                        searchDiv.style.top = "72px";
                    }
                    var searchInput = document.getElementById("search");
                    if (searchInput) {
                        searchInput.focus();
                    }
                }
            }

            if (event.target.matches(".closeSearch")) {
                event.preventDefault();
                var searchInput = document.getElementById("search");
                if (searchInput) {
                    searchInput.value = "";
                }
                var searchDiv = document.querySelector("div.search");
                if (searchDiv) {
                    searchDiv.style.top = "-100px";
                }
            }
        });

        window.addEventListener("load", function () {
            var wrapperElement = document.querySelector(".wrapper");
            if (wrapperElement) {
                var remainingWidth = window.innerWidth - (wrapperElement.offsetLeft + wrapperElement.offsetWidth);
                var mainNav = document.querySelector(".main-nav.opened");
                if (mainNav) {
                    mainNav.style.right = remainingWidth + "px";
                }
            }
        });

        window.addEventListener("resize", function () {
            var wrapperElement = document.querySelector(".wrapper");
            if (wrapperElement) {
                var remainingWidth = window.innerWidth - (wrapperElement.offsetLeft + wrapperElement.offsetWidth);
                var mainNav = document.querySelector(".main-nav.opened");
                if (mainNav) {
                    mainNav.style.right = remainingWidth + "px";
                }

                var searchDiv = document.querySelector("div.search");
                if (searchDiv) {
                    searchDiv.style.right = remainingWidth + "px";
                }
            }
        });

        window.addEventListener("load", function () {
            if (typeof sliderInteriors === "function") {
                sliderInteriors();
            }
        });

        window.addEventListener("resize", function () {
            if (typeof sliderInteriors === "function") {
                sliderInteriors();
            }
        });
    </script>
