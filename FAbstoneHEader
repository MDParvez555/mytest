 <!-- <script src="https://www.sociinc.com/wp-content/themes/soci/assets/js/min/app.min.js"></script> -->
 <script >
function headerWidth() {
    var wrapper = document.querySelector("div.wrapper");
    var header = document.querySelector("header");

    if (wrapper && header) {
        var t = wrapper.offsetWidth;
        header.style.marginLeft = (-t / 2) + "px";
    }
}

document.addEventListener("DOMContentLoaded", function () {
    headerWidth();
});

window.addEventListener("resize", function () {
    headerWidth();
});

document.addEventListener("click", function (event) {
    if (event.target.matches("a.openMenu")) {
        event.preventDefault();
        var e = event.target;
        e.classList.add("activated");
        var n = e.getAttribute("data-close");

        var img = e.querySelector("img");
        if (img) img.setAttribute("src", n);

        var wrapper = document.querySelector(".wrapper");
        if (wrapper) {
            var i = window.innerWidth - (wrapper.offsetLeft + wrapper.offsetWidth);
            document.querySelector(".nav-closer").style.display = "block";
            var mainNav = document.querySelector(".main-nav");
            if (mainNav) {
                mainNav.style.right = i + "px";
                mainNav.style.top = "72px";
                mainNav.classList.add("opened");
            }
        }
    }

    if (event.target.matches("a.openMenu.activated, .nav-closer")) {
        event.preventDefault();
        var e = document.querySelector("a.openMenu.activated");
        if (e) {
            e.classList.remove("activated");
            var n = e.getAttribute("data-open");

            var img = e.querySelector("img");
            if (img) img.setAttribute("src", n);
        }
        
        document.querySelector(".nav-closer").style.display = "none";
        var mainNav = document.querySelector(".main-nav");
        if (mainNav) {
            mainNav.style.top = "-100%";
        }
    }

    if (event.target.matches("a.openSearch")) {
        event.preventDefault();
        var wrapper = document.querySelector(".wrapper");
        if (wrapper) {
            var n = window.innerWidth - (wrapper.offsetLeft + wrapper.offsetWidth);
            var searchDiv = document.querySelector("div.search");
            if (searchDiv) {
                searchDiv.style.right = n + "px";
                searchDiv.style.top = "72px";
            }
            var searchInput = document.getElementById("search");
            if (searchInput) {
                searchInput.focus();
            }
        }
    }

    if (event.target.matches(".closeSearch")) {
        event.preventDefault();
        var searchInput = document.getElementById("search");
        if (searchInput) {
            searchInput.value = "";
        }
        var searchDiv = document.querySelector("div.search");
        if (searchDiv) {
            searchDiv.style.top = "-100px";
        }
    }
});

window.addEventListener("load", function () {
    var wrapper = document.querySelector(".wrapper");
    if (wrapper) {
        var t = window.innerWidth - (wrapper.offsetLeft + wrapper.offsetWidth);
        var mainNav = document.querySelector(".main-nav.opened");
        if (mainNav) {
            mainNav.style.right = t + "px";
        }
    }
});

window.addEventListener("resize", function () {
    var wrapper = document.querySelector(".wrapper");
    if (wrapper) {
        var t = window.innerWidth - (wrapper.offsetLeft + wrapper.offsetWidth);
        var mainNav = document.querySelector(".main-nav.opened");
        if (mainNav) {
            mainNav.style.right = t + "px";
        }

        var searchDiv = document.querySelector("div.search");
        if (searchDiv) {
            searchDiv.style.right = t + "px";
        }
    }
});

window.addEventListener("load", function () {
    if (typeof sliderInteriors === "function") {
        sliderInteriors();
    }
});

window.addEventListener("resize", function () {
    if (typeof sliderInteriors === "function") {
        sliderInteriors();
    }
});

        
</script> 

