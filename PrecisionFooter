    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>

    <script>

(function ($) {
    function loadImagesAsync(container) {
        container.each(function () {
            const $container = $(this);
            const imageWidth = $container.data("image-width");
            const resolution = $container.data("image-resolution");

            const images = $container.find("li").filter(function () {
                return $(this).data("media-id");
            }).map(function () {
                const $el = $(this);
                return {
                    mediaId: $el.data("media-id"),
                    nonce: $el.data("nonce"),
                    regenerate: $el.is('[data-regenerate-thumbnails]')
                };
            }).toArray();

            function loadNext(images) {
                if (!images.length) return;
                const image = images.shift();

                wp.ajax.post("wpzoom_instagram_get_image_async", {
                    "media-id": image.mediaId,
                    nonce: image.nonce,
                    "image-resolution": resolution,
                    "image-width": imageWidth,
                    "regenerate-thumbnails": image.regenerate
                }).done(function (response) {
                    $container.find(`li[data-media-id="${image.mediaId}"] .zoom-instagram-link`)
                        .css("background-image", `url(${response.image_src})`);
                }).always(function () {
                    loadNext(images);
                });
            }

            loadNext(images);
        });
    }

    function applyGridLayout(container) {
        container.each(function () {
            const $this = $(this);
            const perRow = $this.data("images-per-row");
            const imgWidth = $this.data("image-width");
            const spacing = $this.data("image-spacing");
            const totalWidth = $this.width();

            const columns = totalWidth / imgWidth < perRow ? perRow :
                Math.floor((totalWidth - 1) / imgWidth);
            const finalSize = Math.floor((totalWidth - 1 - (columns - 1) * spacing) / columns);

            $this.find("li").each(function (i) {
                $(this).css({
                    clear: (i % columns === 0) ? "left" : "none",
                    "margin-right": (i % columns === columns - 1) ? 0 : `${spacing}px`,
                    "margin-bottom": `${spacing}px`
                });
            });

            $this.find("a.zoom-instagram-link").css({
                width: finalSize,
                height: finalSize
            });

            $this.removeClass("zoom-instagram-widget__items--no-js");
        });
    }

    function initLightbox(container) {
        container.each(function () {
            const $widget = $(this).closest(".widget");
            const $lightboxWrapper = $widget.find(".wpz-insta-lightbox-wrapper > .swiper");

            if ($lightboxWrapper.length) {
                const swiper = new Swiper($lightboxWrapper.get(0), {
                    lazy: true,
                    loop: false,
                    spaceBetween: 20,
                    watchOverflow: true,
                    navigation: {
                        nextEl: $lightboxWrapper.find("> .swiper-button-next").get(0),
                        prevEl: $lightboxWrapper.find("> .swiper-button-prev").get(0)
                    },
                    keyboard: { enabled: true, onlyInViewport: true }
                });

                $(this).find(".zoom-instagram-link").magnificPopup({
                    items: {
                        src: $widget.find(".wpz-insta-lightbox-wrapper"),
                        type: "inline"
                    },
                    mainClass: "wpzoom-lightbox",
                    midClick: true,
                    callbacks: {
                        open: function () {
                            const $popup = $.magnificPopup.instance.st.el;
                            const uid = $popup.data("mfp-src");
                            const slideIndex = this.content.find(`[data-uid="${uid}"]`).index();
                            swiper.slideTo(slideIndex);
                        }
                    }
                });
            }
        });
    }

    function initializeInstagramWidget() {
        const $widgets = $(".zoom-instagram-widget__items");

        applyGridLayout($widgets);
        loadImagesAsync($widgets);

        if ($widgets.filter('[data-lightbox="1"]').length) {
            initLightbox($widgets.filter('[data-lightbox="1"]'));
        }
    }

    $(window).on("load resize orientationchange", initializeInstagramWidget);
    $(document).on("panels_setup_preview", _.debounce(initializeInstagramWidget, 1500));

    $(window).on("elementor/frontend/init", function () {
        elementorFrontend.hooks.addAction("frontend/element_ready/widget", function ($scope) {
            if ($scope.data("widget_type") === "wpzoom-elementor-instagram-widget.default") {
                initializeInstagramWidget();
            }
        });
    });
})(jQuery);
(function($) {
    $(document).ready(function() {
        $('.zoom-instagram-link[data-mfp-src]').magnificPopup({
            type: 'image',
            mainClass: 'wpzoom-lightbox',
            closeOnContentClick: true,
            closeBtnInside: false,
            removalDelay: 300,
            
            // Customize the HTML structure that gets added to DOM
            markup: `
                <div class="mfp-bg"></div>
                <div class="mfp-wrap">
                    <div class="mfp-container">
                        <div class="mfp-content">
                            <div class="mfp-figure">
                                <img class="mfp-img" src="{{src}}">
                                <div class="mfp-bottom-bar">
                                    <div class="mfp-title">{{title}}</div>
                                    <div class="mfp-social">
                                        <a href="{{el.attr('href')}}" target="_blank" class="mfp-instagram-link">
                                            <i class="icon-instagram"></i> View on Instagram
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <button title="Close (Esc)" type="button" class="mfp-close"></button>
                    </div>
                </div>
            `,
            
            // Image settings
            image: {
                titleSrc: function(item) {
                    // Use the link's title attribute + Instagram link
                    return item.el.attr('title');
                },
                verticalFit: true
            },
            
            // Callbacks
            callbacks: {
                beforeOpen: function() {
                    // Fade in animation
                    this.st.bgOpacity = 0.9;
                },
                open: function() {
                    console.log('Lightbox opened');
                },
                close: function() {
                    console.log('Lightbox closed');
                },
                imageLoadComplete: function() {
                    var container = this.content.find('.mfp-container');
                    setTimeout(function() {
                        container.addClass('mfp-loaded');
                    }, 16);
                }
            }
        });
    });
})(jQuery);





