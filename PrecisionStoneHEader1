<script>
    class MobileMenu {
  constructor() {
    this.body = document.body;
    this.window = window;
    this.header = document.getElementById('nm-header');
    this.mobileMenuBtn = document.getElementById('nm-mobile-menu-button');
    this.mobileMenu = document.getElementById('nm-mobile-menu');
    this.mobileMenuScroller = this.mobileMenu?.querySelector('.nm-mobile-menu-scroll');
    this.pageOverlay = document.getElementById('nm-page-overlay');
    this.classes = {
      menuOpen: 'mobile-menu-open',
      overlayShow: 'show',
      menuOverlay: 'nm-mobile-menu-overlay',
      activeItem: 'active',
      openSubmenu: 'open',
      overlayFadeOut: 'fade-out'
    };
    this.scrollbarWidth = this.getScrollbarWidth();
    this.animationSpeed = 250; // ms
    this.desktopBreakpoint = 992; // px
    if (this.mobileMenu && this.mobileMenuBtn) {
      this.init();
    }
  }
  
  init() {
    this.prepareMenu();
    this.bindEvents();
    this.window.addEventListener('resize', this.debounce(() => {
      this.handleResize();
    }, 250));
    this.handleMediaQueryChange();
  }
  bindEvents() {
    this.mobileMenuBtn.addEventListener('click', (e) => {
      e.preventDefault();
      this.toggleMenu();
    });
    const menuItems = this.mobileMenu.querySelectorAll('ul li.menu-item');
    menuItems.forEach(item => {
      item.addEventListener('click', (e) => {
        if (e.target.tagName === 'A' && !e.target.classList.contains('nm-menu-toggle')) {
          return;
        }
        const submenu = item.querySelector('ul');
        if (submenu) {
          e.stopPropagation();
          e.preventDefault();
          item.classList.toggle(this.classes.activeItem);
          submenu.classList.toggle(this.classes.openSubmenu);
        }
      });
    });
    this.pageOverlay.addEventListener('click', () => {
      this.closeMenu(true);
    });
  }
  prepareMenu() {
    const headerHeight = this.header.offsetHeight;
    const windowHeight = this.window.innerHeight;
    const menuHeight = windowHeight - headerHeight;
    this.mobileMenuScroller.style.maxHeight = `${menuHeight}px`;
    this.mobileMenuScroller.style.marginRight = `-${this.scrollbarWidth}px`;
  }
  toggleMenu() {
    if (this.isMenuOpen()) {
      this.closeMenu(true);
    } else {
      this.openMenu();
    }
  }
  openMenu() {
    const headerHeight = this.header.offsetHeight;
    this.mobileMenuScroller.style.marginTop = `${headerHeight}px`;
    this.body.classList.add(this.classes.menuOpen);
    this.pageOverlay.classList.add(this.classes.overlayShow, this.classes.menuOverlay);
    this.addMediaQueryListener();
  }
  closeMenu(withOverlay = true) {
    this.body.classList.remove(this.classes.menuOpen);
    if (withOverlay) {
      this.pageOverlay.classList.add(this.classes.overlayFadeOut);
      setTimeout(() => {
        this.pageOverlay.classList.remove(
          this.classes.overlayShow, 
          this.classes.menuOverlay,
          this.classes.overlayFadeOut
        );
      }, this.animationSpeed);
    }
    setTimeout(() => {
      this.resetMenuState();
    }, this.animationSpeed);
  }
  resetMenuState() {
    const activeItems = this.mobileMenu.querySelectorAll(`.${this.classes.activeItem}`);
    activeItems.forEach(item => {
      item.classList.remove(this.classes.activeItem);
      const submenu = item.querySelector('ul');
      if (submenu) submenu.classList.remove(this.classes.openSubmenu);
    });
  }
  handleResize() {
    this.prepareMenu();
    this.handleMediaQueryChange();
  }
  
  handleMediaQueryChange() {
    if (window.innerWidth >= this.desktopBreakpoint && this.isMenuOpen()) {
      this.closeMenu(true);
    }
  }
  
  addMediaQueryListener() {
    const mediaQuery = window.matchMedia(`(min-width: ${this.desktopBreakpoint}px)`);
    
    const handler = (e) => {
      if (e.matches && this.isMenuOpen()) {
        this.closeMenu(true);
      }
    };
    
    if (mediaQuery.addEventListener) {
      mediaQuery.addEventListener('change', handler);
    } else {
      mediaQuery.addListener(handler); // Fallback for older browsers
    }
    
    return () => {
      if (mediaQuery.removeEventListener) {
        mediaQuery.removeEventListener('change', handler);
      } else {
        mediaQuery.removeListener(handler);
      }
    };
  }
  
  isMenuOpen() {
    return this.body.classList.contains(this.classes.menuOpen);
  }
  
  getScrollbarWidth() {
    const scrollDiv = document.createElement('div');
    scrollDiv.style.cssText = 'width: 99px; height: 99px; overflow: scroll; position: absolute; top: -9999px;';
    document.body.appendChild(scrollDiv);
    const scrollbarWidth = scrollDiv.offsetWidth - scrollDiv.clientWidth;
    document.body.removeChild(scrollDiv);
    return scrollbarWidth;
  }
  
  debounce(func, wait) {
    let timeout;
    return function() {
      const context = this, args = arguments;
      clearTimeout(timeout);
      timeout = setTimeout(() => {
        func.apply(context, args);
      }, wait);
    };
  }
}

document.addEventListener('DOMContentLoaded', () => {
  new MobileMenu();
});
a.$mobileMenuLi.on("click.nmMenuToggle", function(e) {
    e.stopPropagation();
    a.$document.trigger("nm_mobile_menu_toggle", [e, this]);
    var g = b(this)
      , l = g.children("ul");
    !l.length || g.hasClass("nm-notoggle") && !b(e.target).hasClass("nm-menu-toggle") || (e.preventDefault(),
    g.toggleClass("active"),
    l.toggleClass("open"))
});
</script>

</html>
