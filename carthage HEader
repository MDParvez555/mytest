    <script>
        (function ($) {
            var resizeId;
            $(window).resize(function () {
                stopTransition();
            });

            function stopTransition() {
                $('.container').addClass('notransition');
                clearTimeout(resizeId);
                resizeId = setTimeout(doneResizing, 500);
            }

            function doneResizing() {
                offsetCalc();
            }

            window.addEventListener('DOMContentLoaded', () => {
                smoothScroll();
                offsetCalc();
                scrollTopCheck();
            });

            /*smooth scroll*/
            function smoothScroll() {
                $('a[href*="#"]:not([href="#"])').click(function () {
                    if ($('.menu-button').css('display') == 'block')
                        var fixedOffset = $("nav.main-navigation").height();
                    else
                        var fixedOffset = $("header.masthead").height();

                    if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname && !$(this).hasClass('no-scroll')) {
                        var target = $(this.hash);
                        target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
                        if (target.length) {
                            $('html,body').animate({
                                scrollTop: target.offset().top - fixedOffset
                            }, 800);
                            return false;
                        }
                    }
                });
            }

            /*controls*/
            $(document).ready(function () {
                /*menu*/
                var menubutton = '<button class="menu-control menu-open" type="button"><span class="menu-label">Menu</span></button>';
                $('.main-navigation').before(menubutton);

                var closebutton = '<button class="menu-control menu-close" type="button"><span class="menu-label">Close</span></button>';
                $('.main-navigation').prepend(closebutton);

                $('.menu-control.menu-open').on('click', function () {
                    $('.container').addClass('nav-active');
                    $('body').addClass('overlay-open');
                });
                $('.menu-control.menu-close').on('click', function () {
                    $('.container').removeClass('nav-active');
                    $('body').removeClass('overlay-open');
                });

                $('.menu-item-holder > a').on('click', function (e) {
                    e.preventDefault();
                });
                $('.main, .main-navigation .menu > .menu-item > a').on('click', function () {
                    if (($('.menu-control').css('display') == 'block') && ($('.container').hasClass('nav-active')) && (!$(this).parent().hasClass('menu-item-has-children'))) {
                        $('.container').removeClass('nav-active');
                        $('body').removeClass('overlay-open');
                    }
                });

                /*submenu*/
                /*$('.main-navigation .menu-item-has-children > a').on('click', function(e){
                    if($('.menu-control').css('display') == 'block') {
                        e.preventDefault();
                        $(this).siblings('.sub-menu').slideToggle();
                        $(this).parent().toggleClass('sub-active');
                    }
                });*/
                $(".main-navigation .menu > .menu-item-has-children > a").each(function () {
                    $(this).after('<span class="sub-control" role="button" tabindex="0"></span>');
                });
                $('.main-navigation .sub-control, .main-navigation .menu > .menu-item-has-children > a[href^="#"]').on('click', function (e) {
                    e.preventDefault();

                    if (($('.menu-control').css('display') == 'none')) {
                        if ($(this).parent().hasClass('sub-active')) {
                            $(this).parent().removeClass('sub-active');
                        } else {
                            $('.sub-active').removeClass('sub-active');
                            $(this).parent().addClass('sub-active');
                        }
                    } else {
                        $(this).siblings('.sub-menu').slideToggle();
                        $(this).parent().toggleClass('sub-active');
                    }
                });

                /*faq*/
                $(".csw-accordion .csw-faq-question").on('click', function () {
                    $(this).toggleClass('csw-faq-active');
                    $(this).siblings('.csw-faq-answer').slideToggle();
                });

                /*resizer*/
                $('.csw-resizer-button').on('click', function (e) {
                    e.preventDefault();
                    var fontsize = parseInt($('.container').css('font-size'));

                    if ($(this).hasClass('csw-resizer-inc') && (fontsize < 30)) {
                        $('.container').css('font-size', "+=2");
                    } else if ($(this).hasClass('csw-resizer-dec') && (fontsize > 12)) {
                        $('.container').css('font-size', "-=2");
                    }
                });

                /*event rsvp
                $( ".single-tribe_events .tribe-events-single-event-description.tribe-events-content" ).each(function() {
                    $(this).append('<p><a class="csw-btn" href="#rsvp"><span>RSVP</span></a></p>');
                });*/

                /*popups*/
                $('.popup-video, .popup-youtube, .popup-vimeo, .popup-gmaps').magnificPopup({
                    type: 'iframe',
                    mainClass: 'mfp-fade',
                    removalDelay: 160,
                    preloader: false,
                    fixedContentPos: false
                });
                $('.csw-gallery-grid').magnificPopup({
                    delegate: 'a',
                    type: 'image',
                    gallery: {
                        enabled: true
                    }
                });
            });

            $(window).load(function () {
                /*sliders*/
                $('.csw-slider.slickslider').slick({
                    centerMode: false,
                    variableWidth: false,
                    arrows: true,
                    centerPadding: '20px',
                    slidesToShow: 3,
                    responsive: [
                        {
                            breakpoint: 1200,
                            settings: {
                                slidesToShow: 2
                            }
                        },
                        {
                            breakpoint: 768,
                            settings: {

                                slidesToShow: 1
                            }
                        }
                    ]
                });
                /*$( '.csw-slider .slickslider' ).slick( {
                    arrows: true,
                    dots:   false
                } );*/
            });


            var didScroll;
            var delta = 50;
            var navbarHeight = $('header.masthead').outerHeight();
            var lastScrollTop = window.pageYOffset || document.documentElement.scrollTop;
            $(window).scroll(function (event) {
                didScroll = true;
            });
            setInterval(function () {
                if (didScroll) {
                    scrollTopCheck();
                    didScroll = false;
                }
            }, 100);

            function scrollTopCheck() {
                if ($('.masthead').length > 0) {
                    var st = $(this).scrollTop();

                    if ((st == 0) && $(".container").hasClass("not-top")) {
                        $(".container").removeClass("not-top");
                    } else if ((st != 0) && !$(".container").hasClass("not-top")) {
                        $(".container").addClass("not-top");
                    }

                    if (Math.abs(lastScrollTop - st) <= delta) {
                        return;
                    }

                    if (st > lastScrollTop && st > navbarHeight) {
                        // Scroll Down
                        $('.container').removeClass('scrolling-up').addClass('scrolling-down');
                    } else {
                        // Scroll Up
                        if (st + $(window).height() < $(document).height()) {
                            $('.container').removeClass('scrolling-down').addClass('scrolling-up');
                        }
                    }

                    lastScrollTop = st;
                }
            }

            function offsetCalc() {
                $('.container').addClass('notransition');

                var totalHeight = $('.masthead').height();

                $('.main, .tribe-events-view, .tribe-events-pg-template').css('padding-top', totalHeight + 'px');

                timeoutID = window.setTimeout(function () {
                    $('.container').removeClass('notransition');
                }, 500);
            }

            function doAnimation(selector, action, distance, cb) {
                $(selector).each(function () {
                    var pos = $(this).offset().top;
                    var topOfWindow = $(window).scrollTop();
                    if (pos < topOfWindow + distance) {
                        if (!$(this).hasClass(action)) {
                            $(this).trigger(action);
                        }
                        $(this).addClass(action);

                        if (typeof cb !== 'undefined') {
                            cb(selector);
                        }
                    }
                    /*also check from bottom*/
                    else if (($(window).scrollTop() + $(window).height() + distance) >= $(document).height()) {
                        $(this).addClass(action);
                    }
                });
            }
        })(jQuery);
    </script>
</body>

</html>
