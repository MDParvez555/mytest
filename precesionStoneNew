<script>
  document.addEventListener("DOMContentLoaded", function () {
    function decodeCfEmail(cfemail) {
      let decodedEmail = "";
      let key = parseInt(cfemail.substring(0, 2), 16); // Extract the key
      for (let i = 2; i < cfemail.length; i += 2) {
        let charCode = parseInt(cfemail.substring(i, i + 2), 16) ^ key;
        decodedEmail += String.fromCharCode(charCode);
      }
      return decodedEmail;
    }

    // Find all elements with the Cloudflare email obfuscation class
    document.querySelectorAll(".__cf_email__").forEach(function (el) {
      let encodedEmail = el.getAttribute("data-cfemail");
      if (encodedEmail) {
        let decodedEmail = decodeCfEmail(encodedEmail);
        el.textContent = decodedEmail; // Replace text with actual email
        el.href = "mailto:" + decodedEmail; // Make it a mailto link
      }
    });
  });
</script>










